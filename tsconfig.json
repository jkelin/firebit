{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "experimentalDecorators": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "jsx": "react",
      "lib": ["dom", "es6"]
    },
    "include": [
        "./src/**/*"
    ]
}
